{
  "name": "bookmark-manager",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "pages:preview": "bun run build && wrangler pages dev --compatibility-date=2024-02-08 --live-reload .svelte-kit/cloudflare",
    "pages:deploy": "bun run build && wrangler pages deploy .svelte-kit/cloudflare",
    "pages:stage": "bun run build --sourcemap inline --minify false  && wrangler pages deploy --branch staging .svelte-kit/cloudflare"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240208.0",
    "@simplewebauthn/browser": "^9.0.1",
    "@simplewebauthn/server": "^9.0.2",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/adapter-cloudflare": "^4.1.0",
    "@sveltejs/adapter-static": "^3.0.1",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@types/eslint": "8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.36.0-next.4",
    "postcss": "^8.4.32",
    "postcss-load-config": "^5.0.2",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "svelte": "^5.0.0-next.1",
    "svelte-check": "^3.6.0",
    "tailwindcss": "^3.3.6",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.3",
    "wrangler": "^3.28.1"
  },
  "type": "module",
  "dependencies": {
    "@axiomhq/axiom-node": "^0.12.0",
    "@braintree/sanitize-url": "^7.0.0",
    "@cloudflare/puppeteer": "^0.0.5",
    "@corbado/node-sdk": "^2.0.0",
    "@corbado/types": "^1.1.2",
    "@corbado/web-js": "^1.1.2",
    "@corbado/webcomponent": "^1.0.104",
    "@dqbd/tiktoken": "^1.0.13",
    "@mozilla/readability": "^0.5.0",
    "ai": "^2.2.34",
    "bits-ui": "^0.18.1",
    "clsx": "^2.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "fauna": "^1.3.1",
    "faunadb": "^4.8.0",
    "formsnap": "^0.4.3",
    "github-markdown-css": "^5.5.1",
    "jsdom": "^24.0.0",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0",
    "jwt-decode": "^4.0.0",
    "lit-element": "^4.0.4",
    "lit-html": "^3.1.2",
    "lucide-svelte": "^0.323.0",
    "node-jose": "^2.2.0",
    "openai": "^4.27.0",
    "parse5": "^7.1.2",
    "svelte-highlight": "^7.6.0",
    "svelte-legos": "^0.2.2",
    "svelte-markdown": "^0.4.1",
    "sveltekit-superforms": "^2.0.0",
    "tailwind-merge": "^2.2.1",
    "tailwind-variants": "^0.1.20",
    "tiktoken": "^1.0.13",
    "turndown": "^7.1.2",
    "vaul-svelte": "^0.2.3",
    "vite-plugin-top-level-await": "^1.4.1",
    "vite-plugin-wasm": "^3.3.0"
  }
}
